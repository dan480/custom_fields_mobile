<?xml version="1.0" encoding="utf-8"?>
<odoo>
	
	<record id="product_form" model="ir.ui.view">
		<field name="name">product.form</field>
		<field name="model">mobile.phone.inherit</field>
		<field name="priority" eval="50" />
		<field name="inherit_id" ref="product.product_template_only_form_view"/>
		<field name="arch" type="xml">
			<field name="barcode" position="after">
				<field name="manufacturer" string="Manufacturer" placeholder="Please select"/>
				<field name="model_phone" string="Model" placeholder="Please select"/>
			</field>		
		</field>	
	</record>

</odoo>
